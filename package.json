{
  "name": "leaflet-geotiff-2",
  "description": "A LeafletJS plugin for displaying geoTIFF raster data.",
  "version": "1.1.0",
  "license": "See LICENSE in LICENSE.md",
  "author": "daniel.wild@csiro.au",
  "contributors": [
    "Stuart Matthews",
    "Matthias Mohr",
    "Werner Christian",
    "Anton Savoskin",
    "faroscore"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "type": "module",
  "files": ["dist"],
  "main": "./dist/leaflet-geotiff.cjs",
  "module": "./dist/leaflet-geotiff.js",
  "exports": {
    ".": {
      "import": "./dist/leaflet-geotiff.js",
      "require": "./dist/leaflet-geotiff.cjs"
    },
    "./leaflet-geotiff-rgb": {
      "import": "./dist/leaflet-geotiff-rgb.js",
      "require": "./dist/leaflet-geotiff-rgb.cjs"
    },
    "./leaflet-geotiff-vector-arrows": {
      "import": "./dist/leaflet-geotiff-vector-arrows.js",
      "require": "./dist/leaflet-geotiff-vector-arrows.cjs"
    },
    "./leaflet-geotiff-plotty": {
      "import": "./dist/leaflet-geotiff-plotty.js",
      "require": "./dist/leaflet-geotiff-plotty.cjs"
    }
  },
  "peerDependencies": {
    "geotiff": "^2.1.3",
    "leaflet": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "~5.6.2",
    "vite": "^6.0.5"
  },
  "optionalDependencies": {
    "plotty": "^0.4.9"
  }
}
